{
  "id": "{{PACKAGE_NAME}}",
  "title": "{{MODULE_NAME}}",
  "description": "{{MODULE_DESCRIPTION}}",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 12,
    "verified": 12,
    "maximum": 12
  },
  "authors": [
    {
      "name": "{{AUTHOR_NAME}}",
      "url": "https://github.com/{{GITHUB_OWNER}}"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/refs/tags/release-4.1.1/system.json",
        "compatibility": {
          "verified": "4.1.1"
        }
      }
    ],
    "requires": []
  },
  "esmodules": [
    "dist/module.js"
  ],
  "styles": [
    "dist/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "dist/languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://raw.githubusercontent.com/{{GITHUB_FULLNAME}}/refs/heads/main/icons/logo.png"
    }
  ]
}
